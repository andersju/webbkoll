<section class="result">
  <h3 id="raw-headers">
    <%= gettext "Raw headers" %>&#32;
    <%= anchor_link("raw-headers") %>
  </h3>

  <table>
    <thead>
      <tr>
        <th><%= gettext("Header") %></th>
        <th><%= gettext("Value") %></th>
      </tr>
    </thead>
    <tbody>
      <%= for {header, value} <- @site.data.headers do %>
      <tr>
        <td data-label="Header" class="userdata"><%= header %></td>
        <td data-label="Value" class="userdata"><%= value %></td>
      </tr>
      <% end %>
    </tbody>
  </table>
</section>